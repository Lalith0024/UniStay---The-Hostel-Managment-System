{
  "version": 2,
  "functions": {
    "index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/products/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
